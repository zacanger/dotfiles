[core]
  editor = nvim
  excludesfile = /home/z/Dropbox/z/.gitignore_global
  ignorecase = false
[user]
  email = zac@zacanger.com
  name = zacanger
  signingkey = 0E301CABD9D72DB5
[hub]
  username = zacanger
  oauthtoken =
[push]
  default = matching
[color]
  ui = 1
[alias]
  branches = !legit branches
  graft = !legit graft
  harvest = !legit harvest
  publish = !legit publish
  unpublish = !legit unpublish
  sprout = !legit sprout
  sync = !legit sync
  switch = !legit switch
  resync = !legit resync
  b = log -p -M --follow --stat --
  chart = log --graph --oneline --all --decorate
  graph = log --all --graph --pretty=format:'%Cred%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  ac = !git add -A && git commit
  st = status -sb
  stash = stash -u
  remote = remote -v
  branch = branch -rv
  rebase = rebase -i
  web = instaweb
  add = add -A
  p = push
  clear = checkout .
  rpo = remote prune origin
  alias = config --get-regexp 'alias.*'
  search = "grep _Iin"
  # still working on this
  foo = log --color --graph --abbrev-commit --pretty=format:'%Cred%h%Creset -%C(yellow)%d%n%Creset %s%n%Cgreen(%cr)%Creset by %C(bold blue)<%an>%Creset%Cred %GK%n'
[help]
  autocorrect = 10
[instaweb]
  local = true
  port = 7777
  browser = chromium
[commit]
  template = ~/.gitmessage
  gpgsign = true
[http]
  postBuffer = 524288000
